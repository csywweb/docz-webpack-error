{
  "name": "@youzan/mei-components",
  "description": "美业前端业务组件库",
  "version": "1.2.1",
  "main": "lib/index.js",
  "jsnext:main": "src/index.js",
  "files": [
    "src/",
    "lib/"
  ],
  "scripts": {
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook", 
    "build:site": "node ./node_modules/.bin/build-storybook -o ./dist",
    "package": "node ./build/package.js",
    "clean": "rm -rf lib/ dist/",
    "build": "npm run build:check && npm run clean && npm run build:babel && npm run build:style",
    "build:babel": "babel src/ -d lib --ignore __mocks__,__story__.js,__test__.js,__demo__.js",
    "build:babel:watch": "babel src/ -d lib --ignore __mocks__,__story__.js,__test__.js,__demo__.js --watch",
    "build:style": "node ./build/style.js",
    "build:check": "node ./build/check.js",
    "prepublish": "npm run build",
    "lintjs": "eslint src/"
  },
  "dependencies": {
    "@youzan/react-amap": "^5.0.0",
    "@youzan/region-select": "4.3.8",
    "autoprefixer": "^9.4.3",
    "classnames": "^2.2.6",
    "lodash": "^4.17.5",
    "object-assign": "^4.1.1",
    "prop-types": "^15.6.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "styled-components": "^3.3.3",
    "webpack": "3.5.5",
    "fecha": "^3.0.3",
    "big.js":"^5.2.2",
    "zent": "7.0.0-next.14"
  },
  "devDependencies": {
    "copy-dir": "^0.4.0",
    "replace-in-file": "^3.4.0",
    "fs-extra": "^5.0.0",
    "@storybook/addon-actions": "^3.3.13",
    "@storybook/addon-console": "^1.0.0",
    "@storybook/addon-info": "^3.4.0",
    "@storybook/addon-knobs": "^3.4.0",
    "@storybook/addon-links": "^3.3.13",
    "@storybook/addon-options": "^3.4.0",
    "@storybook/addons": "^3.3.13",
    "@storybook/react": "^3.3.13",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.6",
    "babel-loader": "^7.1.4",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-zent": "^2.0.0-next.5",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "css-loader": "^0.28.11",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-plugin-babel": "^5.1.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.10.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "node-sass": "^4.9.2",
    "postcss-loader": "^3.0.0",
    "sass-loader": "^7.0.3",
    "storybook-readme": "^3.3.0",
    "style-loader": "^0.20.3",
    "webpack": "^3.5.5"
  },
  "repository": {
    "type": "git",
    "url": "git@gitlab.qima-inc.com:sz-web/beauty-component-react.git"
  },
  "keywords": [
    "components",
